{
    "name": "ciricihq/cake-oauth2-client",
    "description": "OAuth2Client plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "OAuth2Client\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OAuth2Client\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
